{
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/js/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}